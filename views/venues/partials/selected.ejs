<div class="card text-start">
    <div class="card-body">
        <h2 class="card-title">
            <%= venue.name %>
        </h2>
        <h3 class="card-subtitle text-muted">
            <%= venue.city %>, <%= venue.state %>
        </h3>

        <h4>
            Power Service:
            <%= venue.power %>
        </h4>
        <h4>
            Union:
            <%= venue.union %>
        </h4>
        
            <div class="dropdown">
                <a class="btn btn-secondary dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                    Events:
                </a>
              
                <ul class="dropdown-menu">
                    
                    <%- venue.events.map(e=>
                    `<li> <a class="dropdown-item" href="/events/${e._id}">${e.name}</a>
                        </li>
                    `
                    ).join('') %>
                </ul>
              </div>
            
        

    </div>

</div>